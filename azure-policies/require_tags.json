{
  "if": {
    "allOf": [
      {
        "field": "type",
        "notIn": [
          "Microsoft.KeyVault/vaults",
          "Microsoft.Resources/subscriptions/resourceGroups"
        ]
      },
      {
        "field": "[concat('tags[', parameters('tagName'), ']')]",
        "exists": false
      }
    ]
  },
  "then": {
    "effect": "deny"
  }
}
